        android:layout_height="597dp"
        app:layout_constraintBottom_toBottomOf="@+id/button_information"
        app:layout_constraintTop_toBottomOf="@+id/button_interview"
        tools:context="직업 선택 메인화면, mbti버튼">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="165dp"
            android:orientation="vertical"
            tools:layout_editor_absoluteX="2dp"
            tools:layout_editor_absoluteY="400dp">

            <Space
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />

            <Button
                android:id="@+id/button_interview"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"

                android:background="@drawable/user_record_ripple"
                android:text="직업정보"
                android:textColor="@color/black" />

            <Button
                android:id="@+id/button_information"
                android:layout_width="match_parent"
                android:layout_height="0dp"
                android:layout_weight="1"
                android:background="@drawable/user_record_ripple"
                android:text="직업인 인터뷰"
                android:textColor="@color/black" />
        </LinearLayout>


    </androidx.constraintlayout.widget.ConstraintLayout><![CDATA[





        />



