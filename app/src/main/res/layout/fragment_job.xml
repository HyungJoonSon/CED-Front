            android:text="ESFJ"
            android:textColor="@color/black"
            tools:layout_editor_absoluteX="113dp"
            tools:layout_editor_absoluteY="290dp" />

        <Button
            android:id="@+id/button_ENFJ"
            android:layout_width="wrap_content"
