<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <LinearLayout
        android:id="@+id/pragjob"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingBottom="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/bmhanna"
            android:text="Job"
            android:textColor="@color/black"
            android:textSize="40dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/layoutmbti"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical|center_horizontal"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingBottom="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pragjob">

        <TextView
            android:id="@+id/textmbti"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/nbgothic"
            android:text="MBTI"
            android:textAlignment="center"
            android:textColor="@color/black"
            android:textSize="30dp"
            android:textStyle="bold" />
    </LinearLayout>

    <View
        android:id="@+id/view1"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:background="#a0a0a0"
        app:layout_constraintBottom_toBottomOf="@+id/layoutmbti"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/mbti_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:orientation="vertical"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingBottom="10dp"
        app:layout_constraintTop_toBottomOf="@+id/layoutmbti">

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button_ISTJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ISTJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ISFJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ISFJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_INFJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="INFJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />


            <Button
                android:id="@+id/button_INTJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="INTJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button_ISTP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ISTP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ISFP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ISFP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_INFP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="INFP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />


            <Button
                android:id="@+id/button_INTP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="INTP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button_ESTP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ESTP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ESFP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ESFP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ENFP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ENFP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ENTP"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ENTP"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal">

            <Button
                android:id="@+id/button_ESTJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ESTJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ESFJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ESFJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ENFJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ENFJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

            <Button
                android:id="@+id/button_ENTJ"
                android:layout_width="95dp"
                android:layout_height="70dp"
                android:background="@drawable/user_record_ripple"
                android:fontFamily="@font/nbgothic"
                android:text="ENTJ"
                android:textColor="@color/black"
                android:textSize="20dp"
                android:textStyle="bold" />

        </LinearLayout>
    </LinearLayout>

    <View
        android:id="@+id/view2"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:background="#a0a0a0"
        app:layout_constraintBottom_toBottomOf="@+id/mbti_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/jobdata_btn_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:fontFamily="@font/nbgothic"
        android:gravity="center_vertical|center_horizontal"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingBottom="10dp"
        app:layout_constraintTop_toBottomOf="@+id/view2">

        <Button
            android:id="@+id/button_information"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/rank_stopwatch_ripple"
            android:fontFamily="@font/nbgothic"
            android:text="직업정보"
            android:textColor="@color/black"
            android:textSize="25dp" />

    </LinearLayout>

    <View
        android:id="@+id/view3"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:background="#a0a0a0"
        app:layout_constraintBottom_toBottomOf="@+id/jobdata_btn_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/jobinterview_btn_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical|center_horizontal"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingBottom="10dp"
        app:layout_constraintTop_toBottomOf="@+id/jobdata_btn_layout">

        <Button
            android:id="@+id/button_interview"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="@drawable/rank_stopwatch_ripple"
            android:fontFamily="@font/nbgothic"
            android:text="직업인 인터뷰"
            android:textColor="@color/black"
            android:textSize="25dp" />
    </LinearLayout>

    <View
        android:id="@+id/view4"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginStart="15dp"
        android:layout_marginEnd="15dp"
        android:background="#a0a0a0"
        app:layout_constraintBottom_toBottomOf="@+id/jobinterview_btn_layout"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>