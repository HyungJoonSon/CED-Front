<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.InterviewActivity">

    <LinearLayout
        android:id="@+id/job_interview_detailTop"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingLeft="15dp"
        android:paddingTop="10dp"
        android:paddingRight="15dp"
        android:paddingBottom="10dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:id="@+id/InterviewTop"
            android:layout_width="300dp"
            android:layout_height="wrap_content"
            android:fontFamily="@font/bmhanna"
            android:gravity="center_vertical|left"
            android:text="직업 인터뷰"
            android:textColor="@color/black"
            android:textSize="35dp" />

        <View
            android:layout_width="40dp"
            android:layout_height="0dp" />

        <ImageButton
            android:id="@+id/jobinterviewBack"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:background="@drawable/setting_ripple"
            android:outlineAmbientShadowColor="@color/black"
            android:src="@drawable/ic_baseline_clear" />
    </LinearLayout>

    <WebView
        android:id="@+id/interviewWeb"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="60dp"
        app:layout_constraintTop_toBottomOf="@+id/job_interview_detailTop" />

</androidx.constraintlayout.widget.ConstraintLayout>